{
    "name": "mirko-pagliai/me-cms-instagram",
    "description": "Instagram plugin for MeCms",
    "homepage": "https://github.com/mirko-pagliai/me-cms-instagram",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [{
            "name": "Mirko Pagliai",
            "email": "mirko.pagliai@gmail.com"
        }],
    "require": {
        "php": ">=7.0",
        "mirko-pagliai/me-cms": "^2.27.3"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^3.0",
        "mirko-pagliai/cakephp-assets": "^1.4.1",
        "mirko-pagliai/php-thumber": "~1.0.0",
        "mirko-pagliai/php-tools": "~1.2.0",
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "MeCmsInstagram\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MeCmsInstagram\\Test\\": "tests",
            "App\\": "tests/test_app/TestApp"
        }
    }
}
