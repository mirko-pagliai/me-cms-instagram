{
    "name": "mirko-pagliai/me-cms-instagram",
    "description": "Instagram plugin for MeCms",
    "homepage": "https://github.com/mirko-pagliai/me-cms-instagram",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [{
        "name": "Mirko Pagliai",
        "email": "mirko.pagliai@gmail.com"
    }],
    "config": {
        "component-dir": "vendor/components"
    },
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.4.1 <3.5",
        "mirko-pagliai/me-cms": "dev-develop"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^3.0",
        "mirko-pagliai/reflection": "^1.0",
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "MeCmsInstagram\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MeCmsInstagram\\Test\\": "tests",
            "App\\": "tests/test_app/TestApp"
        }
    }
}