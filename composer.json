{
    "name": "mirko-pagliai/me-cms-instagram",
    "description": "Instagram plugin for MeCms",
    "homepage": "https://github.com/mirko-pagliai/me-cms-instagram",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [{
        "name": "Mirko Pagliai",
        "email": "mirko.pagliai@gmail.com"
    }],
    "config": {
        "component-dir": "vendor/components"
    },
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": "^3.7.1",
        "mirko-pagliai/me-cms": "^2.25",
        "mirko-pagliai/php-tools": "^1.1.7"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^3.0",
        "mirko-pagliai/cakephp-assets": "^1.3",
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "MeCmsInstagram\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MeCmsInstagram\\Test\\": "tests",
            "App\\": "tests/test_app/TestApp"
        }
    }
}
