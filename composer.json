{
    "name": "mirko-pagliai/me-cms-instagram",
    "description": "Instagram plugin for MeCms",
    "homepage": "https://github.com/mirko-pagliai/me-cms-instagram",
    "type": "cakephp-plugin",
    "license": "AGPL",
    "authors": [{
        "name": "Mirko Pagliai",
        "email": "mirko.pagliai@gmail.com"
    }],
    "config": {
        "component-dir": "vendor/components"
    },
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.4 <4.0",
        "mirko-pagliai/me-cms": "dev-2.15version"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "2.*",
        "mirko-pagliai/reflection": "*"
    },
    "autoload": {
        "psr-4": {
            "MeCmsInstagram\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MeCmsInstagram\\Test\\": "tests",
            "App\\": "tests/test_app/TestApp"
        }
    },
    "minimum-stability": "dev"
}